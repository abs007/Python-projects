while True:
    count = 1
    num=0
    lst=[]
    for n in range(0,count+1):
        num+=n
        count+=1
        for i in range(1,num+1):
            if num%i==0:
                lst.append(i)
                if len(lst)>500:
                    print("It's the {}th number".format(num))
                    break
